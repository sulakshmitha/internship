6. ascii conv
public class sample6
{
 public static void main(String arg[])
    {
        char s='d';
        s=(char)(s+3);
        System.out.println(s);
    }
}


7. addition 
public class sample7{  
    public static void main(String args[])  
    {  
    double a=6.2;  
    int b=5;
    double c=a+b;  
        System.out.println("addition :"+c);  
     }  
}

8. square
  public class sample8{  
